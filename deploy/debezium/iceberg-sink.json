{
  "name": "iceberg-sink",
  "config": {
    "connector.class": "org.apache.iceberg.kafka.connect.IcebergSinkConnector",
    "tasks.max": "1",
    "topics.regex": "analytics.public.*",
    "catalog.name": "lakehouse",
    "catalog.type": "hadoop",
    "catalog.hadoop.warehouse": "s3://lakehouse/warehouse",
    "s3.endpoint": "http://minio:9000",
    "s3.access.key": "minioadmin",
    "s3.secret.key": "minioadmin"
  }
}
